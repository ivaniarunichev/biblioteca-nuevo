pipeline {
    agent any

    tools {
        jdk 'jdk-17'
        maven 'Maven3.9.11'
    }

    stages {
        stage('Show messages'){
            steps {
                echo "Primer stage del pipeline"
            }
        }
        stage('Checkout proyecto'){
                            steps {
                                git branch: 'main',
                                    url: 'https://github.com/ivaniarunichev/biblioteca-nuevo.git'
                            }
                }
    }

}